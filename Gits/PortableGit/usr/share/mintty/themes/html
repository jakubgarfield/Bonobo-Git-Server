# https://en.wikipedia.org/wiki/Web_colors#HTML_color_names

#	Black	#000000	(black)
Black         = #000000
#	Maroon	#800000	(low red)
Red           = #800000
#	Green	#008000	(low green)
Green         = #008000
#	Olive	#808000	(brown)
Yellow        = #808000
#	Navy	#000080	(low blue)
Blue          = #000080
#	Purple	#800080	(low magenta)
Magenta       = #800080
#	Teal	#008080	(low cyan)
Cyan          = #008080
#	Silver	#C0C0C0	(light gray)
White         = #C0C0C0

#	Gray	#808080	(dark gray)
BoldBlack     = #808080
#	Red	#FF0000	(high red)
BoldRed       = #FF0000
#	Lime	#00FF00	(high green); green
BoldGreen     = #00FF00
#	Yellow	#FFFF00	(yellow)
BoldYellow    = #FFFF00
#	Blue	#0000FF	(high blue)
BoldBlue      = #0000FF
#	Fuchsia	#FF00FF	(high magenta); magenta
BoldMagenta   = #FF00FF
#	Aqua	#00FFFF	(high cyan); cyan
BoldCyan      = #00FFFF
#	White	#FFFFFF	(white)
BoldWhite     = #FFFFFF
