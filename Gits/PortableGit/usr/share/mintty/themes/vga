# https://en.wikipedia.org/wiki/Video_Graphics_Array#Color_palette

Black		= #000000
Blue		= #0000aa
Green		= #00aa00
Cyan		= #00aaaa
Red		= #aa0000
Magenta		= #aa00aa
# brown:
Yellow		= #aa5500
# gray:
White		= #aaaaaa

# dark gray:
BoldBlack	= #555555
# bright blue:
BoldBlue	= #5555ff
# bright green:
BoldGreen	= #55ff55
# bright cyan:
BoldCyan	= #55ffff
# bright red:
BoldRed		= #ff5555
# bright magenta:
BoldMagenta	= #ff55ff
# yellow:
BoldYellow	= #ffff55
# white:
BoldWhite	= #ffffff
